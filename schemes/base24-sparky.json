          "AttributeValueColor": [255, 143, 28],
          "CdataSectionColor": [70, 152, 203],
          "CharacterColor": [45, 204, 211],
          "CommentColor": [0, 59, 73],
          "DoctypeDeclarationColor": [120, 214, 75],
          "ErrorColor": [155, 112, 77],
          "OperatorColor": [213, 158, 215],
          "ProcessingInstructionColor": [120, 214, 75],
          "TagColor": [255, 88, 93]
        },
        "java": {
          "CharactersColor": [45, 204, 211],
          "CommentColor": [0, 59, 73],
          "KeywordColor": [213, 158, 215],
          "StringsColor": [120, 214, 75]
        },
        "tlc": {
          "CommentColor": [0, 59, 73],
          "KeywordColor": [213, 158, 215],
          "MacroColor": [255, 143, 28],
          "StringsColor": [120, 214, 75]
