{
  "ColorSchemeName": "Decaf",
  "ColorSchemeAuthor": "Alex Mirrington (https://github.com/alexmirrington)",
  "Note": "Apply this theme in Matlab using the tinted_import function, available at https://github.com/tinted-theming/tinted-matlab",

  "matlab": {
    "colors": {
      "ForegroundColor": [204, 204, 204],
      "BackgroundColor": [45, 45, 45],
      "KeywordColor": [239, 179, 247],
      "CommentColor": [119, 119, 119],
      "StringColor": [190, 218, 120],
      "UnterminatedStringColor": [255, 147, 179],
      "SystemCommandColor": [190, 214, 255],
      "SyntaxErrorColor": [255, 127, 123],
      "commandwindow": {
        "HyperlinkColor": [144, 190, 225],
        "ErrorColor": [255, 127, 123],
        "WarningColor": [255, 214, 124]
