{
  "ColorSchemeName": "Marrakesh",
  "ColorSchemeAuthor": "Alexandre Gavioli (http://github.com/Alexx2/)",
  "Note": "Apply this theme in Matlab using the tinted_import function, available at https://github.com/tinted-theming/tinted-matlab",

  "matlab": {
    "colors": {
      "ForegroundColor": [148, 142, 72],
      "BackgroundColor": [32, 22, 2],
      "KeywordColor": [136, 104, 179],
      "CommentColor": [108, 104, 35],
      "StringColor": [24, 151, 78],
      "UnterminatedStringColor": [179, 88, 142],
      "SystemCommandColor": [117, 167, 56],
      "SyntaxErrorColor": [195, 83, 89],
      "commandwindow": {
        "HyperlinkColor": [71, 124, 161],
        "ErrorColor": [195, 83, 89],
        "WarningColor": [168, 131, 57]
      },
      "programmingtools": {
        "CodeAnalyzerWarningColor": [168, 131, 57],
        "AutofixHighlightColor": [179, 88, 142],
        "VariableHighlightColor": [95, 91, 23],
        "VariablesWithSharedScopeColor": [179, 97, 68]
      }
    },
    "editor": {
