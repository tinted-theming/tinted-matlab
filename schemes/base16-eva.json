{
  "ColorSchemeName": "Eva",
  "ColorSchemeAuthor": "kjakapat (https://github.com/kjakapat)",
  "Note": "Apply this theme in Matlab using the tinted_import function, available at https://github.com/tinted-theming/tinted-matlab",

  "matlab": {
    "colors": {
      "ForegroundColor": [159, 162, 166],
      "BackgroundColor": [42, 59, 77],
