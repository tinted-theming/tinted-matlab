{
  "ColorSchemeName": "Mathias",
  "ColorSchemeAuthor": "FredHappyface (https://github.com/fredHappyface)",
  "Note": "Apply this theme in Matlab using the tinted_import function, available at https://github.com/tinted-theming/tinted-matlab",

  "matlab": {
    "colors": {
      "ForegroundColor": [202, 202, 202],
      "BackgroundColor": [0, 0, 0],
      "KeywordColor": [250, 37, 115],
      "CommentColor": [124, 124, 124],
      "StringColor": [166, 227, 45],
      "UnterminatedStringColor": [114, 17, 17],
      "SystemCommandColor": [103, 217, 240],
      "SyntaxErrorColor": [229, 34, 34],
      "commandwindow": {
        "HyperlinkColor": [196, 141, 255],
        "ErrorColor": [229, 34, 34],
        "WarningColor": [85, 85, 255]
      },
      "programmingtools": {
        "CodeAnalyzerWarningColor": [85, 85, 255],
        "AutofixHighlightColor": [114, 17, 17],
        "VariableHighlightColor": [85, 85, 85],
        "VariablesWithSharedScopeColor": [252, 149, 30]
      }
    },
    "editor": {
      "displaysettings": {
        "HighlightCurrentLineColor": [0, 0, 0],
        "linelimit": {
          "LineColor": [85, 85, 85]
        }
      },
      "language": {
        "cpp": {
          "BadCharactersColor": [114, 17, 17],
          "CharactersColor": [103, 217, 240],
