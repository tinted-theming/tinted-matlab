{
  "ColorSchemeName": "Highway",
  "ColorSchemeAuthor": "FredHappyface (https://github.com/fredHappyface)",
  "Note": "Apply this theme in Matlab using the tinted_import function, available at https://github.com/tinted-theming/tinted-matlab",

  "matlab": {
    "colors": {
      "ForegroundColor": [200, 197, 196],
      "BackgroundColor": [33, 34, 36],
      "KeywordColor": [106, 38, 116],
      "CommentColor": [128, 118, 114],
      "StringColor": [18, 128, 51],
      "UnterminatedStringColor": [103, 6, 11],
      "SystemCommandColor": [56, 69, 99],
      "SyntaxErrorColor": [207, 13, 23],
      "commandwindow": {
