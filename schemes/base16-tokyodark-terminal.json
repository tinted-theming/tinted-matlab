{
  "ColorSchemeName": "Tokyodark Terminal",
  "ColorSchemeAuthor": "Tiagovla (https://github.com/tiagovla/)",
  "Note": "Apply this theme in Matlab using the tinted_import function, available at https://github.com/tinted-theming/tinted-matlab",

  "matlab": {
    "colors": {
      "ForegroundColor": [160, 168, 205],
      "BackgroundColor": [17, 18, 29],
      "KeywordColor": [164, 133, 221],
      "CommentColor": [40, 44, 52],
      "StringColor": [149, 197, 97],
      "UnterminatedStringColor": [119, 52, 64],
      "SystemCommandColor": [56, 168, 157],
      "SyntaxErrorColor": [238, 109, 133],
      "commandwindow": {
        "HyperlinkColor": [113, 153, 238],
