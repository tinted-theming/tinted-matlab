{
  "ColorSchemeName": "Grape",
  "ColorSchemeAuthor": "FredHappyface (https://github.com/fredHappyface)",
  "Note": "Apply this theme in Matlab using the tinted_import function, available at https://github.com/tinted-theming/tinted-matlab",

  "matlab": {
    "colors": {
      "ForegroundColor": [140, 138, 146],
      "BackgroundColor": [22, 20, 35],
      "KeywordColor": [140, 53, 200],
      "CommentColor": [105, 99, 119],
      "StringColor": [31, 169, 27],
      "UnterminatedStringColor": [118, 16, 48],
      "SystemCommandColor": [58, 221, 237],
      "SyntaxErrorColor": [236, 33, 96],
      "commandwindow": {
        "HyperlinkColor": [72, 124, 244],
        "ErrorColor": [236, 33, 96],
        "WarningColor": [169, 187, 235]
      },
      "programmingtools": {
        "CodeAnalyzerWarningColor": [169, 187, 235],
        "AutofixHighlightColor": [118, 16, 48],
        "VariableHighlightColor": [88, 80, 106],
        "VariablesWithSharedScopeColor": [141, 220, 31]
      }
    },
    "editor": {
      "displaysettings": {
        "HighlightCurrentLineColor": [45, 40, 62],
