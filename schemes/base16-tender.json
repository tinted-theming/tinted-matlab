{
  "ColorSchemeName": "tender",
  "ColorSchemeAuthor": "Jacobo Tabernero (https://github/com/jacoborus/tender.vim)",
  "Note": "Apply this theme in Matlab using the tinted_import function, available at https://github.com/tinted-theming/tinted-matlab",

  "matlab": {
    "colors": {
      "ForegroundColor": [238, 238, 238],
      "BackgroundColor": [40, 40, 40],
      "KeywordColor": [211, 185, 135],
      "CommentColor": [76, 76, 76],
      "StringColor": [201, 208, 92],
      "UnterminatedStringColor": [161, 105, 70],
      "SystemCommandColor": [115, 206, 244],
      "SyntaxErrorColor": [244, 55, 83],
      "commandwindow": {
