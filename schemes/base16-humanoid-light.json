{
  "ColorSchemeName": "Humanoid light",
  "ColorSchemeAuthor": "Thomas (tasmo) Friese",
  "Note": "Apply this theme in Matlab using the tinted_import function, available at https://github.com/tinted-theming/tinted-matlab",

  "matlab": {
    "colors": {
      "ForegroundColor": [35, 38, 41],
      "BackgroundColor": [248, 248, 242],
      "KeywordColor": [112, 15, 152],
      "CommentColor": [192, 192, 189],
