{
  "ColorSchemeName": "Cobalt Neon",
  "ColorSchemeAuthor": "FredHappyface (https://github.com/fredHappyface)",
  "Note": "Apply this theme in Matlab using the tinted_import function, available at https://github.com/tinted-theming/tinted-matlab",

  "matlab": {
    "colors": {
      "ForegroundColor": [204, 114, 166],
      "BackgroundColor": [20, 40, 56],
      "KeywordColor": [120, 26, 160],
      "CommentColor": [238, 202, 146],
      "StringColor": [58, 165, 255],
      "UnterminatedStringColor": [127, 17, 16],
      "SystemCommandColor": [143, 245, 134],
      "SyntaxErrorColor": [255, 35, 32],
      "commandwindow": {
        "HyperlinkColor": [143, 245, 134],
        "ErrorColor": [255, 35, 32],
        "WarningColor": [60, 125, 210]
      },
      "programmingtools": {
        "CodeAnalyzerWarningColor": [60, 125, 210],
        "AutofixHighlightColor": [255, 246, 136],
        "VariableHighlightColor": [255, 246, 136],
        "VariablesWithSharedScopeColor": [233, 231, 92]
      }
    },
    "editor": {
