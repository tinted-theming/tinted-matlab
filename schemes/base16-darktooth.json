{
  "ColorSchemeName": "Darktooth",
  "ColorSchemeAuthor": "Jason Milkins (https://github.com/jasonm23)",
  "Note": "Apply this theme in Matlab using the tinted_import function, available at https://github.com/tinted-theming/tinted-matlab",

  "matlab": {
    "colors": {
      "ForegroundColor": [168, 153, 132],
      "BackgroundColor": [29, 32, 33],
