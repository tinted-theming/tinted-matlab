{
  "ColorSchemeName": "Flexoki Light",
  "ColorSchemeAuthor": "Steph Ango (https://github.com/kepano/flexoki)",
  "Note": "Apply this theme in Matlab using the tinted_import function, available at https://github.com/tinted-theming/tinted-matlab",

  "matlab": {
    "colors": {
      "ForegroundColor": [64, 62, 60],
      "BackgroundColor": [255, 252, 240],
      "KeywordColor": [94, 64, 157],
      "CommentColor": [206, 205, 195],
      "StringColor": [102, 128, 11],
      "UnterminatedStringColor": [160, 47, 111],
      "SystemCommandColor": [36, 131, 123],
      "SyntaxErrorColor": [175, 48, 41],
      "commandwindow": {
        "HyperlinkColor": [32, 94, 166],
        "ErrorColor": [175, 48, 41],
        "WarningColor": [173, 131, 1]
      },
      "programmingtools": {
        "CodeAnalyzerWarningColor": [173, 131, 1],
        "AutofixHighlightColor": [173, 131, 1],
        "VariableHighlightColor": [36, 131, 123],
        "VariablesWithSharedScopeColor": [188, 82, 21]
      }
    },
    "editor": {
      "displaysettings": {
        "HighlightCurrentLineColor": [242, 240, 229],
        "linelimit": {
          "LineColor": [230, 228, 217]
        }
      },
      "language": {
        "cpp": {
          "BadCharactersColor": [160, 47, 111],
          "CharactersColor": [36, 131, 123],
          "CommentColor": [206, 205, 195],
          "KeywordColor": [94, 64, 157],
          "PreprocessorColor": [188, 82, 21],
          "StringsColor": [102, 128, 11]
        },
        "html": {
          "AttributeNameColor": [188, 82, 21],
          "AttributeValueColor": [188, 82, 21],
          "CdataSectionColor": [32, 94, 166],
          "CharacterColor": [36, 131, 123],
          "CommentColor": [206, 205, 195],
          "DoctypeDeclarationColor": [102, 128, 11],
          "ErrorColor": [160, 47, 111],
          "OperatorColor": [94, 64, 157],
          "ProcessingInstructionColor": [102, 128, 11],
          "TagColor": [175, 48, 41]
        },
        "java": {
          "CharactersColor": [36, 131, 123],
          "CommentColor": [206, 205, 195],
          "KeywordColor": [94, 64, 157],
          "StringsColor": [102, 128, 11]
        },
        "tlc": {
          "CommentColor": [206, 205, 195],
          "KeywordColor": [94, 64, 157],
          "MacroColor": [188, 82, 21],
          "StringsColor": [102, 128, 11]
        },
        "verilog": {
          "CommentColor": [206, 205, 195],
          "KeywordColor": [94, 64, 157],
          "OperatorColor": [188, 82, 21],
          "StringColor": [102, 128, 11]
        },
        "vhdl": {
          "CommentColor": [206, 205, 195],
          "KeywordColor": [94, 64, 157],
          "OperatorColor": [188, 82, 21],
          "StringColor": [102, 128, 11]
        }
      }
    },
    "appdesigner": {
      "codeview": {
        "ReadonlyBackgroundColor": [230, 228, 217]
      }
    }
  }
}
