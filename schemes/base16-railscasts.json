{
  "ColorSchemeName": "Railscasts",
  "ColorSchemeAuthor": "Ryan Bates (http://railscasts.com)",
  "Note": "Apply this theme in Matlab using the tinted_import function, available at https://github.com/tinted-theming/tinted-matlab",

  "matlab": {
    "colors": {
      "ForegroundColor": [230, 225, 220],
      "BackgroundColor": [43, 43, 43],
      "KeywordColor": [182, 179, 235],
      "CommentColor": [90, 100, 126],
      "StringColor": [165, 194, 97],
      "UnterminatedStringColor": [188, 148, 88],
      "SystemCommandColor": [81, 159, 80],
      "SyntaxErrorColor": [218, 73, 57],
      "commandwindow": {
        "HyperlinkColor": [109, 156, 190],
        "ErrorColor": [218, 73, 57],
        "WarningColor": [255, 198, 109]
      },
      "programmingtools": {
        "CodeAnalyzerWarningColor": [255, 198, 109],
        "AutofixHighlightColor": [255, 198, 109],
        "VariableHighlightColor": [81, 159, 80],
        "VariablesWithSharedScopeColor": [204, 120, 51]
      }
    },
    "editor": {
