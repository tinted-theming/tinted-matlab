          "CharacterColor": [142, 192, 124],
          "CommentColor": [102, 92, 84],
          "DoctypeDeclarationColor": [184, 187, 38],
          "ErrorColor": [214, 93, 14],
          "OperatorColor": [211, 134, 155],
          "ProcessingInstructionColor": [184, 187, 38],
          "TagColor": [251, 73, 52]
        },
        "java": {
          "CharactersColor": [142, 192, 124],
          "CommentColor": [102, 92, 84],
          "KeywordColor": [211, 134, 155],
          "StringsColor": [184, 187, 38]
        },
        "tlc": {
          "CommentColor": [102, 92, 84],
