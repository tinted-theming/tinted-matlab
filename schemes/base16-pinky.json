{
  "ColorSchemeName": "pinky",
  "ColorSchemeAuthor": "Benjamin (https://github.com/b3nj5m1n)",
  "Note": "Apply this theme in Matlab using the tinted_import function, available at https://github.com/tinted-theming/tinted-matlab",

  "matlab": {
    "colors": {
      "ForegroundColor": [245, 245, 245],
      "BackgroundColor": [23, 21, 23],
      "KeywordColor": [0, 127, 255],
      "CommentColor": [56, 51, 56],
      "StringColor": [255, 0, 102],
      "UnterminatedStringColor": [223, 32, 108],
      "SystemCommandColor": [102, 0, 255],
      "SyntaxErrorColor": [255, 166, 0],
      "commandwindow": {
        "HyperlinkColor": [0, 255, 255],
        "ErrorColor": [255, 166, 0],
        "WarningColor": [32, 223, 108]
      },
      "programmingtools": {
        "CodeAnalyzerWarningColor": [32, 223, 108],
        "AutofixHighlightColor": [32, 223, 108],
        "VariableHighlightColor": [102, 0, 255],
        "VariablesWithSharedScopeColor": [0, 255, 102]
      }
    },
    "editor": {
      "displaysettings": {
        "HighlightCurrentLineColor": [27, 24, 27],
        "linelimit": {
          "LineColor": [29, 27, 29]
        }
      },
      "language": {
        "cpp": {
