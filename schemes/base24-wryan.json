{
  "ColorSchemeName": "Wryan",
  "ColorSchemeAuthor": "FredHappyface (https://github.com/fredHappyface)",
  "Note": "Apply this theme in Matlab using the tinted_import function, available at https://github.com/tinted-theming/tinted-matlab",

  "matlab": {
    "colors": {
      "ForegroundColor": [118, 132, 136],
      "BackgroundColor": [16, 16, 16],
      "KeywordColor": [94, 70, 140],
      "CommentColor": [80, 84, 86],
      "StringColor": [40, 115, 115],
      "UnterminatedStringColor": [70, 35, 50],
      "SystemCommandColor": [49, 101, 140],
      "SyntaxErrorColor": [140, 70, 101],
      "commandwindow": {
        "HyperlinkColor": [57, 85, 115],
        "ErrorColor": [140, 70, 101],
        "WarningColor": [71, 122, 179]
      },
      "programmingtools": {
        "CodeAnalyzerWarningColor": [71, 122, 179],
        "AutofixHighlightColor": [71, 122, 179],
        "VariableHighlightColor": [49, 101, 140],
        "VariablesWithSharedScopeColor": [124, 124, 153]
      }
    },
    "editor": {
      "displaysettings": {
        "HighlightCurrentLineColor": [51, 51, 51],
        "linelimit": {
