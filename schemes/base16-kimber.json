{
  "ColorSchemeName": "Kimber",
  "ColorSchemeAuthor": "Mishka Nguyen (https://github.com/akhsiM)",
  "Note": "Apply this theme in Matlab using the tinted_import function, available at https://github.com/tinted-theming/tinted-matlab",

  "matlab": {
    "colors": {
      "ForegroundColor": [222, 222, 231],
      "BackgroundColor": [34, 34, 34],
      "KeywordColor": [134, 202, 205],
      "CommentColor": [100, 70, 70],
