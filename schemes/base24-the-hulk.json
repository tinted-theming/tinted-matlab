          "CharacterColor": [55, 140, 169],
          "CommentColor": [114, 116, 115],
          "DoctypeDeclarationColor": [19, 206, 47],
          "ErrorColor": [18, 78, 13],
          "OperatorColor": [100, 30, 115],
          "ProcessingInstructionColor": [19, 206, 47],
          "TagColor": [37, 157, 26]
