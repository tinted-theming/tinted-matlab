{
  "ColorSchemeName": "Isotope",
  "ColorSchemeAuthor": "Jan T. Sott",
  "Note": "Apply this theme in Matlab using the tinted_import function, available at https://github.com/tinted-theming/tinted-matlab",

  "matlab": {
    "colors": {
      "ForegroundColor": [208, 208, 208],
      "BackgroundColor": [0, 0, 0],
      "KeywordColor": [204, 0, 255],
      "CommentColor": [128, 128, 128],
      "StringColor": [51, 255, 0],
      "UnterminatedStringColor": [51, 0, 255],
      "SystemCommandColor": [0, 255, 255],
      "SyntaxErrorColor": [255, 0, 0],
      "commandwindow": {
        "HyperlinkColor": [0, 102, 255],
        "ErrorColor": [255, 0, 0],
        "WarningColor": [255, 0, 153]
      },
      "programmingtools": {
        "CodeAnalyzerWarningColor": [255, 0, 153],
        "AutofixHighlightColor": [255, 0, 153],
        "VariableHighlightColor": [0, 255, 255],
        "VariablesWithSharedScopeColor": [255, 153, 0]
      }
    },
    "editor": {
      "displaysettings": {
        "HighlightCurrentLineColor": [64, 64, 64],
        "linelimit": {
