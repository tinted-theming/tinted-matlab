        "WarningColor": [242, 255, 39]
      },
      "programmingtools": {
        "CodeAnalyzerWarningColor": [242, 255, 39],
        "AutofixHighlightColor": [242, 255, 39],
        "VariableHighlightColor": [90, 235, 188],
        "VariablesWithSharedScopeColor": [251, 161, 27]
      }
    },
    "editor": {
