{
  "ColorSchemeName": "Gruvbox dark, soft",
  "ColorSchemeAuthor": "Dawid Kurek (dawikur@gmail.com), morhetz (https://github.com/morhetz/gruvbox)",
  "Note": "Apply this theme in Matlab using the tinted_import function, available at https://github.com/tinted-theming/tinted-matlab",

  "matlab": {
    "colors": {
      "ForegroundColor": [213, 196, 161],
      "BackgroundColor": [50, 48, 47],
      "KeywordColor": [211, 134, 155],
      "CommentColor": [102, 92, 84],
      "StringColor": [184, 187, 38],
      "UnterminatedStringColor": [214, 93, 14],
      "SystemCommandColor": [142, 192, 124],
      "SyntaxErrorColor": [251, 73, 52],
      "commandwindow": {
        "HyperlinkColor": [131, 165, 152],
        "ErrorColor": [251, 73, 52],
        "WarningColor": [250, 189, 47]
      },
      "programmingtools": {
        "CodeAnalyzerWarningColor": [250, 189, 47],
        "AutofixHighlightColor": [250, 189, 47],
        "VariableHighlightColor": [142, 192, 124],
        "VariablesWithSharedScopeColor": [254, 128, 25]
      }
    },
    "editor": {
      "displaysettings": {
        "HighlightCurrentLineColor": [60, 56, 54],
        "linelimit": {
          "LineColor": [80, 73, 69]
        }
      },
      "language": {
