{
  "ColorSchemeName": "Summerfruit Light",
  "ColorSchemeAuthor": "Christopher Corley (http://christop.club/)",
  "Note": "Apply this theme in Matlab using the tinted_import function, available at https://github.com/tinted-theming/tinted-matlab",

  "matlab": {
    "colors": {
      "ForegroundColor": [16, 16, 16],
      "BackgroundColor": [255, 255, 255],
