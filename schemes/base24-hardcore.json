        "vhdl": {
          "CommentColor": [99, 99, 99],
          "KeywordColor": [170, 136, 255],
          "OperatorColor": [255, 204, 51],
          "StringColor": [152, 236, 101]
        }
      }
    },
    "appdesigner": {
      "codeview": {
        "ReadonlyBackgroundColor": [65, 65, 65]
      }
    }
  }
}
