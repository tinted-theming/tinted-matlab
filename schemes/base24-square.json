{
  "ColorSchemeName": "Square",
  "ColorSchemeAuthor": "FredHappyface (https://github.com/fredHappyface)",
  "Note": "Apply this theme in Matlab using the tinted_import function, available at https://github.com/tinted-theming/tinted-matlab",

  "matlab": {
    "colors": {
      "ForegroundColor": [186, 186, 186],
      "BackgroundColor": [26, 26, 26],
      "KeywordColor": [117, 80, 123],
      "CommentColor": [75, 75, 75],
      "StringColor": [182, 55, 125],
      "UnterminatedStringColor": [116, 68, 62],
      "SystemCommandColor": [201, 202, 236],
      "SyntaxErrorColor": [233, 137, 124],
      "commandwindow": {
        "HyperlinkColor": [169, 205, 235],
        "ErrorColor": [233, 137, 124],
        "WarningColor": [182, 222, 251]
      },
      "programmingtools": {
        "CodeAnalyzerWarningColor": [182, 222, 251],
        "AutofixHighlightColor": [20, 20, 20],
        "VariableHighlightColor": [20, 20, 20],
        "VariablesWithSharedScopeColor": [236, 235, 190]
      }
    },
    "editor": {
