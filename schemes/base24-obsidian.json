{
  "ColorSchemeName": "Obsidian",
  "ColorSchemeAuthor": "FredHappyface (https://github.com/fredHappyface)",
  "Note": "Apply this theme in Matlab using the tinted_import function, available at https://github.com/tinted-theming/tinted-matlab",

  "matlab": {
    "colors": {
      "ForegroundColor": [161, 161, 161],
      "BackgroundColor": [39, 48, 50],
      "KeywordColor": [187, 0, 187],
      "CommentColor": [110, 110, 110],
      "StringColor": [0, 187, 0],
      "UnterminatedStringColor": [82, 0, 0],
      "SystemCommandColor": [0, 187, 187],
      "SyntaxErrorColor": [165, 0, 1],
      "commandwindow": {
        "HyperlinkColor": [57, 155, 218],
        "ErrorColor": [165, 0, 1],
        "WarningColor": [160, 214, 255]
      },
      "programmingtools": {
        "CodeAnalyzerWarningColor": [160, 214, 255],
        "AutofixHighlightColor": [160, 214, 255],
        "VariableHighlightColor": [0, 187, 187],
        "VariablesWithSharedScopeColor": [254, 204, 34]
      }
    },
    "editor": {
