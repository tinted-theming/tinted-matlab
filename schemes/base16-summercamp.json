{
  "ColorSchemeName": "summercamp",
  "ColorSchemeAuthor": "zoe firi (zoefiri.github.io)",
  "Note": "Apply this theme in Matlab using the tinted_import function, available at https://github.com/tinted-theming/tinted-matlab",

  "matlab": {
    "colors": {
      "ForegroundColor": [115, 110, 85],
      "BackgroundColor": [28, 24, 16],
      "KeywordColor": [255, 128, 128],
      "CommentColor": [80, 75, 56],
      "StringColor": [92, 235, 90],
      "UnterminatedStringColor": [246, 155, 231],
      "SystemCommandColor": [90, 235, 188],
      "SyntaxErrorColor": [227, 81, 66],
