{
  "ColorSchemeName": "Shades of Purple",
  "ColorSchemeAuthor": "Iolar Demartini Junior (http://github.com/demartini), based on Shades of Purple Theme (https://github.com/ahmadawais/shades-of-purple-vscode)",
  "Note": "Apply this theme in Matlab using the tinted_import function, available at https://github.com/tinted-theming/tinted-matlab",

  "matlab": {
    "colors": {
      "ForegroundColor": [199, 199, 199],
      "BackgroundColor": [30, 30, 63],
      "KeywordColor": [255, 44, 112],
      "CommentColor": [128, 128, 128],
      "StringColor": [58, 217, 0],
      "UnterminatedStringColor": [121, 232, 251],
      "SystemCommandColor": [0, 197, 199],
      "SyntaxErrorColor": [217, 4, 41],
      "commandwindow": {
        "HyperlinkColor": [105, 67, 255],
        "ErrorColor": [217, 4, 41],
        "WarningColor": [255, 231, 0]
      },
      "programmingtools": {
        "CodeAnalyzerWarningColor": [255, 231, 0],
        "AutofixHighlightColor": [255, 231, 0],
        "VariableHighlightColor": [0, 197, 199],
        "VariablesWithSharedScopeColor": [249, 42, 28]
      }
    },
    "editor": {
