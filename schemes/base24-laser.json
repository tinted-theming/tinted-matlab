{
  "ColorSchemeName": "Laser",
  "ColorSchemeAuthor": "FredHappyface (https://github.com/fredHappyface)",
  "Note": "Apply this theme in Matlab using the tinted_import function, available at https://github.com/tinted-theming/tinted-matlab",

  "matlab": {
    "colors": {
      "ForegroundColor": [216, 216, 216],
      "BackgroundColor": [3, 13, 24],
      "KeywordColor": [255, 143, 253],
      "CommentColor": [166, 166, 166],
      "StringColor": [180, 250, 114],
      "UnterminatedStringColor": [127, 65, 57],
      "SystemCommandColor": [208, 209, 254],
      "SyntaxErrorColor": [255, 130, 114],
      "commandwindow": {
        "HyperlinkColor": [254, 211, 0],
        "ErrorColor": [255, 130, 114],
        "WarningColor": [249, 40, 131]
      },
      "programmingtools": {
        "CodeAnalyzerWarningColor": [249, 40, 131],
        "AutofixHighlightColor": [127, 65, 57],
        "VariableHighlightColor": [142, 142, 142],
        "VariablesWithSharedScopeColor": [9, 180, 189]
      }
    },
    "editor": {
      "displaysettings": {
        "HighlightCurrentLineColor": [97, 97, 97],
