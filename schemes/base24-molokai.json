{
  "ColorSchemeName": "Molokai",
  "ColorSchemeAuthor": "FredHappyface (https://github.com/fredHappyface)",
  "Note": "Apply this theme in Matlab using the tinted_import function, available at https://github.com/tinted-theming/tinted-matlab",

  "matlab": {
    "colors": {
      "ForegroundColor": [161, 161, 161],
      "BackgroundColor": [18, 18, 18],
      "KeywordColor": [135, 0, 255],
      "CommentColor": [110, 110, 110],
      "StringColor": [151, 225, 35],
      "UnterminatedStringColor": [125, 18, 57],
      "SystemCommandColor": [66, 167, 207],
      "SyntaxErrorColor": [250, 37, 115],
      "commandwindow": {
        "HyperlinkColor": [15, 127, 207],
        "ErrorColor": [250, 37, 115],
        "WarningColor": [0, 175, 255]
      },
      "programmingtools": {
        "CodeAnalyzerWarningColor": [0, 175, 255],
        "AutofixHighlightColor": [125, 18, 57],
        "VariableHighlightColor": [85, 85, 85],
        "VariablesWithSharedScopeColor": [223, 212, 96]
      }
    },
    "editor": {
