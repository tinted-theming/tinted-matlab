          "CommentColor": [128, 128, 128],
          "DoctypeDeclarationColor": [51, 255, 0],
          "ErrorColor": [51, 0, 255],
          "OperatorColor": [204, 0, 255],
          "ProcessingInstructionColor": [51, 255, 0],
          "TagColor": [255, 0, 0]
        },
        "java": {
          "CharactersColor": [0, 255, 255],
          "CommentColor": [128, 128, 128],
          "KeywordColor": [204, 0, 255],
          "StringsColor": [51, 255, 0]
