{
  "ColorSchemeName": "Still Alive",
  "ColorSchemeAuthor": "Derrick McKee (derrick.mckee@gmail.com), Tinted Theming (https://github.com/tinted-theming)",
  "Note": "Apply this theme in Matlab using the tinted_import function, available at https://github.com/tinted-theming/tinted-matlab",

  "matlab": {
    "colors": {
      "ForegroundColor": [76, 56, 59],
      "BackgroundColor": [240, 240, 240],
      "KeywordColor": [144, 54, 255],
      "CommentColor": [163, 163, 163],
      "StringColor": [48, 168, 96],
      "UnterminatedStringColor": [20, 12, 13],
      "SystemCommandColor": [54, 211, 255],
      "SyntaxErrorColor": [216, 0, 0],
      "commandwindow": {
        "HyperlinkColor": [54, 94, 255],
        "ErrorColor": [216, 0, 0],
        "WarningColor": [255, 240, 24]
      },
      "programmingtools": {
        "CodeAnalyzerWarningColor": [255, 240, 24],
        "AutofixHighlightColor": [255, 240, 24],
        "VariableHighlightColor": [54, 211, 255],
        "VariablesWithSharedScopeColor": [240, 216, 72]
      }
    },
    "editor": {
