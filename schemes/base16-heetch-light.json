{
  "ColorSchemeName": "Heetch Light",
  "ColorSchemeAuthor": "Geoffrey Teale (tealeg@gmail.com), Tinted Theming (https://github.com/tinted-theming)",
  "Note": "Apply this theme in Matlab using the tinted_import function, available at https://github.com/tinted-theming/tinted-matlab",

  "matlab": {
    "colors": {
      "ForegroundColor": [90, 73, 110],
      "BackgroundColor": [254, 255, 255],
      "KeywordColor": [143, 108, 151],
      "CommentColor": [156, 146, 168],
      "StringColor": [91, 182, 106],
      "UnterminatedStringColor": [88, 66, 93],
      "SystemCommandColor": [71, 249, 245],
      "SyntaxErrorColor": [248, 0, 89],
      "commandwindow": {
