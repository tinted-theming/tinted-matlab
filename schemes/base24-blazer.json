{
  "ColorSchemeName": "Blazer",
  "ColorSchemeAuthor": "FredHappyface (https://github.com/fredHappyface)",
  "Note": "Apply this theme in Matlab using the tinted_import function, available at https://github.com/tinted-theming/tinted-matlab",

  "matlab": {
    "colors": {
      "ForegroundColor": [172, 172, 172],
      "BackgroundColor": [13, 25, 38],
      "KeywordColor": [184, 122, 184],
      "CommentColor": [82, 82, 82],
      "StringColor": [122, 184, 122],
      "UnterminatedStringColor": [92, 61, 61],
      "SystemCommandColor": [122, 184, 184],
      "SyntaxErrorColor": [184, 122, 122],
