        "ErrorColor": [248, 0, 89],
        "WarningColor": [189, 151, 1]
      },
      "programmingtools": {
        "CodeAnalyzerWarningColor": [189, 151, 1],
        "AutofixHighlightColor": [189, 151, 1],
        "VariableHighlightColor": [71, 249, 245],
        "VariablesWithSharedScopeColor": [189, 1, 82]
      }
    },
