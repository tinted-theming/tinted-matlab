{
  "ColorSchemeName": "Outrun Dark",
  "ColorSchemeAuthor": "Hugo Delahousse (http://github.com/hugodelahousse/)",
  "Note": "Apply this theme in Matlab using the tinted_import function, available at https://github.com/tinted-theming/tinted-matlab",

  "matlab": {
    "colors": {
      "ForegroundColor": [208, 208, 250],
      "BackgroundColor": [0, 0, 42],
      "KeywordColor": [241, 5, 150],
      "CommentColor": [80, 80, 122],
      "StringColor": [89, 241, 118],
      "UnterminatedStringColor": [240, 3, 239],
      "SystemCommandColor": [14, 240, 240],
      "SyntaxErrorColor": [255, 66, 66],
      "commandwindow": {
        "HyperlinkColor": [102, 176, 255],
        "ErrorColor": [255, 66, 66],
        "WarningColor": [243, 232, 119]
      },
      "programmingtools": {
        "CodeAnalyzerWarningColor": [243, 232, 119],
        "AutofixHighlightColor": [240, 3, 239],
        "VariableHighlightColor": [48, 48, 90],
        "VariablesWithSharedScopeColor": [252, 141, 40]
      }
    },
    "editor": {
