{
  "ColorSchemeName": "Blueish",
  "ColorSchemeAuthor": "Ben Mayoras",
  "Note": "Apply this theme in Matlab using the tinted_import function, available at https://github.com/tinted-theming/tinted-matlab",

  "matlab": {
    "colors": {
      "ForegroundColor": [200, 225, 248],
      "BackgroundColor": [24, 36, 48],
      "KeywordColor": [255, 132, 221],
      "CommentColor": [97, 109, 120],
      "StringColor": [195, 232, 141],
      "UnterminatedStringColor": [187, 210, 232],
      "SystemCommandColor": [95, 209, 255],
      "SyntaxErrorColor": [76, 229, 135],
      "commandwindow": {
        "HyperlinkColor": [130, 170, 255],
        "ErrorColor": [76, 229, 135],
        "WarningColor": [130, 170, 255]
      },
      "programmingtools": {
        "CodeAnalyzerWarningColor": [130, 170, 255],
        "AutofixHighlightColor": [130, 170, 255],
        "VariableHighlightColor": [95, 209, 255],
        "VariablesWithSharedScopeColor": [246, 168, 92]
      }
    },
    "editor": {
      "displaysettings": {
        "HighlightCurrentLineColor": [36, 60, 84],
        "linelimit": {
          "LineColor": [70, 41, 10]
        }
      },
      "language": {
