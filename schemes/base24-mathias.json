          "AttributeValueColor": [252, 149, 30],
          "CdataSectionColor": [196, 141, 255],
          "CharacterColor": [103, 217, 240],
          "CommentColor": [124, 124, 124],
          "DoctypeDeclarationColor": [166, 227, 45],
          "ErrorColor": [114, 17, 17],
          "OperatorColor": [250, 37, 115],
          "ProcessingInstructionColor": [166, 227, 45],
          "TagColor": [229, 34, 34]
        },
        "java": {
          "CharactersColor": [103, 217, 240],
          "CommentColor": [124, 124, 124],
          "KeywordColor": [250, 37, 115],
          "StringsColor": [166, 227, 45]
        },
        "tlc": {
          "CommentColor": [124, 124, 124],
          "KeywordColor": [250, 37, 115],
