{
  "ColorSchemeName": "Chalkboard",
  "ColorSchemeAuthor": "FredHappyface (https://github.com/fredHappyface)",
  "Note": "Apply this theme in Matlab using the tinted_import function, available at https://github.com/tinted-theming/tinted-matlab",

  "matlab": {
    "colors": {
      "ForegroundColor": [175, 175, 175],
      "BackgroundColor": [41, 38, 47],
      "KeywordColor": [195, 114, 194],
      "CommentColor": [91, 91, 91],
      "StringColor": [114, 195, 115],
      "UnterminatedStringColor": [97, 57, 57],
      "SystemCommandColor": [114, 194, 195],
      "SyntaxErrorColor": [195, 115, 114],
      "commandwindow": {
        "HyperlinkColor": [115, 114, 195],
        "ErrorColor": [195, 115, 114],
        "WarningColor": [170, 170, 219]
      },
      "programmingtools": {
        "CodeAnalyzerWarningColor": [170, 170, 219],
        "AutofixHighlightColor": [50, 50, 50],
        "VariableHighlightColor": [50, 50, 50],
        "VariablesWithSharedScopeColor": [194, 195, 114]
      }
    },
    "editor": {
      "displaysettings": {
        "HighlightCurrentLineColor": [50, 50, 50],
