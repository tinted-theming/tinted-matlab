          "CharacterColor": [102, 0, 255],
          "CommentColor": [56, 51, 56],
          "DoctypeDeclarationColor": [255, 0, 102],
          "ErrorColor": [223, 32, 108],
          "OperatorColor": [0, 127, 255],
          "ProcessingInstructionColor": [255, 0, 102],
          "TagColor": [255, 166, 0]
        },
        "java": {
          "CharactersColor": [102, 0, 255],
          "CommentColor": [56, 51, 56],
          "KeywordColor": [0, 127, 255],
          "StringsColor": [255, 0, 102]
        },
        "tlc": {
          "CommentColor": [56, 51, 56],
          "KeywordColor": [0, 127, 255],
