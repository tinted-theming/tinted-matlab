      "programmingtools": {
        "CodeAnalyzerWarningColor": [255, 214, 124],
        "AutofixHighlightColor": [255, 214, 124],
        "VariableHighlightColor": [190, 214, 255],
        "VariablesWithSharedScopeColor": [255, 191, 112]
      }
    },
    "editor": {
