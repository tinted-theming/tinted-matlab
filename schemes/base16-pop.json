{
  "ColorSchemeName": "Pop",
  "ColorSchemeAuthor": "Chris Kempson (http://chriskempson.com)",
  "Note": "Apply this theme in Matlab using the tinted_import function, available at https://github.com/tinted-theming/tinted-matlab",

  "matlab": {
    "colors": {
      "ForegroundColor": [208, 208, 208],
      "BackgroundColor": [0, 0, 0],
      "KeywordColor": [179, 30, 141],
      "CommentColor": [80, 80, 80],
      "StringColor": [55, 179, 73],
      "UnterminatedStringColor": [122, 45, 0],
      "SystemCommandColor": [0, 170, 187],
      "SyntaxErrorColor": [235, 0, 138],
      "commandwindow": {
