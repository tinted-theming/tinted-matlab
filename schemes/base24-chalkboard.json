          "CharacterColor": [114, 194, 195],
          "CommentColor": [91, 91, 91],
          "DoctypeDeclarationColor": [114, 195, 115],
          "ErrorColor": [97, 57, 57],
          "OperatorColor": [195, 114, 194],
          "ProcessingInstructionColor": [114, 195, 115],
          "TagColor": [195, 115, 114]
        },
        "java": {
          "CharactersColor": [114, 194, 195],
          "CommentColor": [91, 91, 91],
          "KeywordColor": [195, 114, 194],
          "StringsColor": [114, 195, 115]
        },
        "tlc": {
          "CommentColor": [91, 91, 91],
          "KeywordColor": [195, 114, 194],
