          "CharacterColor": [56, 127, 88],
          "CommentColor": [95, 93, 80],
          "DoctypeDeclarationColor": [71, 153, 66],
          "ErrorColor": [75, 20, 7],
          "OperatorColor": [126, 78, 46],
          "ProcessingInstructionColor": [71, 153, 66],
          "TagColor": [151, 40, 15]
        },
        "java": {
          "CharactersColor": [56, 127, 88],
          "CommentColor": [95, 93, 80],
          "KeywordColor": [126, 78, 46],
          "StringsColor": [71, 153, 66]
        },
        "tlc": {
