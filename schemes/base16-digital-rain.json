{
  "ColorSchemeName": "Digital Rain",
  "ColorSchemeAuthor": "Nathan Byrd (https://github.com/cognitivegears)",
  "Note": "Apply this theme in Matlab using the tinted_import function, available at https://github.com/tinted-theming/tinted-matlab",

  "matlab": {
    "colors": {
      "ForegroundColor": [0, 255, 0],
      "BackgroundColor": [0, 0, 0],
      "KeywordColor": [148, 114, 180],
      "CommentColor": [124, 141, 124],
      "StringColor": [100, 200, 60],
      "UnterminatedStringColor": [179, 124, 94],
      "SystemCommandColor": [70, 140, 120],
      "SyntaxErrorColor": [200, 90, 70],
      "commandwindow": {
        "HyperlinkColor": [84, 130, 175],
        "ErrorColor": [200, 90, 70],
        "WarningColor": [166, 122, 80]
