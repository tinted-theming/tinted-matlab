{
  "ColorSchemeName": "Sagelight",
  "ColorSchemeAuthor": "Carter Veldhuizen",
  "Note": "Apply this theme in Matlab using the tinted_import function, available at https://github.com/tinted-theming/tinted-matlab",

  "matlab": {
    "colors": {
      "ForegroundColor": [56, 56, 56],
      "BackgroundColor": [248, 248, 248],
      "KeywordColor": [200, 160, 210],
      "CommentColor": [184, 184, 184],
      "StringColor": [160, 210, 200],
      "UnterminatedStringColor": [210, 178, 160],
      "SystemCommandColor": [162, 214, 245],
      "SyntaxErrorColor": [250, 132, 128],
      "commandwindow": {
        "HyperlinkColor": [160, 167, 210],
        "ErrorColor": [250, 132, 128],
        "WarningColor": [255, 220, 97]
      },
      "programmingtools": {
        "CodeAnalyzerWarningColor": [255, 220, 97],
        "AutofixHighlightColor": [255, 220, 97],
        "VariableHighlightColor": [162, 214, 245],
        "VariablesWithSharedScopeColor": [255, 170, 97]
      }
    },
    "editor": {
      "displaysettings": {
        "HighlightCurrentLineColor": [232, 232, 232],
        "linelimit": {
          "LineColor": [216, 216, 216]
        }
      },
      "language": {
        "cpp": {
