          "CharacterColor": [162, 214, 245],
          "CommentColor": [184, 184, 184],
          "DoctypeDeclarationColor": [160, 210, 200],
          "ErrorColor": [210, 178, 160],
          "OperatorColor": [200, 160, 210],
          "ProcessingInstructionColor": [160, 210, 200],
          "TagColor": [250, 132, 128]
        },
        "java": {
          "CharactersColor": [162, 214, 245],
          "CommentColor": [184, 184, 184],
          "KeywordColor": [200, 160, 210],
          "StringsColor": [160, 210, 200]
        },
        "tlc": {
          "CommentColor": [184, 184, 184],
          "KeywordColor": [200, 160, 210],
