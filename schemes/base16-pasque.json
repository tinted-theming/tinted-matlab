{
  "ColorSchemeName": "Pasque",
  "ColorSchemeAuthor": "Gabriel Fontes (https://github.com/Misterio77)",
  "Note": "Apply this theme in Matlab using the tinted_import function, available at https://github.com/tinted-theming/tinted-matlab",

  "matlab": {
    "colors": {
      "ForegroundColor": [222, 220, 223],
      "BackgroundColor": [39, 28, 58],
      "KeywordColor": [149, 59, 157],
      "CommentColor": [93, 87, 102],
