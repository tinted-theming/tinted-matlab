{
  "ColorSchemeName": "Tango Half Adapted",
  "ColorSchemeAuthor": "FredHappyface (https://github.com/fredHappyface)",
  "Note": "Apply this theme in Matlab using the tinted_import function, available at https://github.com/tinted-theming/tinted-matlab",

  "matlab": {
    "colors": {
      "ForegroundColor": [79, 79, 79],
      "BackgroundColor": [254, 255, 255],
      "KeywordColor": [168, 107, 178],
      "CommentColor": [204, 204, 204],
      "StringColor": [76, 195, 0],
      "UnterminatedStringColor": [127, 0, 0],
      "SystemCommandColor": [0, 189, 195],
      "SyntaxErrorColor": [255, 0, 0],
      "commandwindow": {
        "HyperlinkColor": [0, 141, 245],
        "ErrorColor": [255, 0, 0],
        "WarningColor": [226, 191, 0]
      },
      "programmingtools": {
        "CodeAnalyzerWarningColor": [226, 191, 0],
        "AutofixHighlightColor": [226, 191, 0],
        "VariableHighlightColor": [0, 189, 195],
        "VariablesWithSharedScopeColor": [226, 132, 0]
      }
    },
    "editor": {
      "displaysettings": {
        "HighlightCurrentLineColor": [241, 241, 241],
