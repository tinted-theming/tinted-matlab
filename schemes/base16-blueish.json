          "CharacterColor": [95, 209, 255],
          "CommentColor": [97, 109, 120],
          "DoctypeDeclarationColor": [195, 232, 141],
          "ErrorColor": [187, 210, 232],
          "OperatorColor": [255, 132, 221],
          "ProcessingInstructionColor": [195, 232, 141],
          "TagColor": [76, 229, 135]
        },
        "java": {
          "CharactersColor": [95, 209, 255],
          "CommentColor": [97, 109, 120],
          "KeywordColor": [255, 132, 221],
          "StringsColor": [195, 232, 141]
        },
        "tlc": {
