{
  "ColorSchemeName": "The Hulk",
  "ColorSchemeAuthor": "FredHappyface (https://github.com/fredHappyface)",
  "Note": "Apply this theme in Matlab using the tinted_import function, available at https://github.com/tinted-theming/tinted-matlab",

  "matlab": {
    "colors": {
      "ForegroundColor": [182, 182, 177],
      "BackgroundColor": [27, 29, 30],
      "KeywordColor": [100, 30, 115],
      "CommentColor": [114, 116, 115],
      "StringColor": [19, 206, 47],
      "UnterminatedStringColor": [18, 78, 13],
      "SystemCommandColor": [55, 140, 169],
      "SyntaxErrorColor": [37, 157, 26],
      "commandwindow": {
        "HyperlinkColor": [36, 36, 244],
        "ErrorColor": [37, 157, 26],
        "WarningColor": [79, 106, 149]
      },
      "programmingtools": {
        "CodeAnalyzerWarningColor": [79, 106, 149],
        "AutofixHighlightColor": [18, 78, 13],
        "VariableHighlightColor": [80, 83, 84],
        "VariablesWithSharedScopeColor": [98, 228, 86]
      }
    },
    "editor": {
      "displaysettings": {
        "HighlightCurrentLineColor": [27, 29, 30],
        "linelimit": {
