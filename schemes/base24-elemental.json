{
  "ColorSchemeName": "Elemental",
  "ColorSchemeAuthor": "FredHappyface (https://github.com/fredHappyface)",
  "Note": "Apply this theme in Matlab using the tinted_import function, available at https://github.com/tinted-theming/tinted-matlab",

  "matlab": {
    "colors": {
      "ForegroundColor": [117, 111, 104],
      "BackgroundColor": [33, 33, 28],
      "KeywordColor": [126, 78, 46],
      "CommentColor": [95, 93, 80],
      "StringColor": [71, 153, 66],
      "UnterminatedStringColor": [75, 20, 7],
      "SystemCommandColor": [56, 127, 88],
      "SyntaxErrorColor": [151, 40, 15],
      "commandwindow": {
        "HyperlinkColor": [73, 127, 125],
        "ErrorColor": [151, 40, 15],
        "WarningColor": [120, 216, 216]
      },
      "programmingtools": {
        "CodeAnalyzerWarningColor": [120, 216, 216],
        "AutofixHighlightColor": [75, 20, 7],
        "VariableHighlightColor": [84, 84, 68],
        "VariablesWithSharedScopeColor": [127, 113, 16]
      }
    },
    "editor": {
      "displaysettings": {
        "HighlightCurrentLineColor": [60, 59, 48],
        "linelimit": {
