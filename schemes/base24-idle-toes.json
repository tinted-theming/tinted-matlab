        "vhdl": {
          "CommentColor": [121, 121, 121],
          "KeywordColor": [245, 127, 255],
          "OperatorColor": [255, 198, 109],
          "StringColor": [127, 225, 115]
        }
      }
    },
    "appdesigner": {
      "codeview": {
        "ReadonlyBackgroundColor": [83, 83, 83]
      }
    }
  }
