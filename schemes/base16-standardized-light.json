{
  "ColorSchemeName": "standardized-light",
  "ColorSchemeAuthor": "ali (https://github.com/ali-githb/base16-standardized-scheme)",
  "Note": "Apply this theme in Matlab using the tinted_import function, available at https://github.com/tinted-theming/tinted-matlab",

  "matlab": {
    "colors": {
      "ForegroundColor": [68, 68, 68],
      "BackgroundColor": [255, 255, 255],
      "KeywordColor": [158, 87, 194],
      "CommentColor": [118, 118, 118],
      "StringColor": [49, 134, 31],
      "UnterminatedStringColor": [137, 80, 37],
      "SystemCommandColor": [0, 153, 143],
      "SyntaxErrorColor": [208, 62, 62],
      "commandwindow": {
        "HyperlinkColor": [49, 115, 197],
        "ErrorColor": [208, 62, 62],
        "WarningColor": [173, 130, 0]
      },
      "programmingtools": {
        "CodeAnalyzerWarningColor": [173, 130, 0],
        "AutofixHighlightColor": [137, 80, 37],
        "VariableHighlightColor": [204, 204, 204],
        "VariablesWithSharedScopeColor": [215, 105, 29]
      }
    },
    "editor": {
