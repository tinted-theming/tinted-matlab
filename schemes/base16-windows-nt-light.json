        "vhdl": {
          "CommentColor": [192, 192, 192],
          "KeywordColor": [128, 0, 128],
          "OperatorColor": [255, 255, 0],
          "StringColor": [0, 128, 0]
        }
      }
    },
    "appdesigner": {
      "codeview": {
        "ReadonlyBackgroundColor": [213, 213, 213]
      }
    }
  }
}
