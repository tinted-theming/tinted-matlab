{
  "ColorSchemeName": "Heetch Dark",
  "ColorSchemeAuthor": "Geoffrey Teale (tealeg@gmail.com)",
  "Note": "Apply this theme in Matlab using the tinted_import function, available at https://github.com/tinted-theming/tinted-matlab",

  "matlab": {
    "colors": {
      "ForegroundColor": [189, 182, 197],
      "BackgroundColor": [25, 1, 52],
      "KeywordColor": [130, 3, 76],
      "CommentColor": [123, 109, 139],
      "StringColor": [195, 54, 120],
      "UnterminatedStringColor": [71, 5, 70],
      "SystemCommandColor": [248, 0, 89],
      "SyntaxErrorColor": [39, 217, 213],
      "commandwindow": {
        "HyperlinkColor": [189, 1, 82],
        "ErrorColor": [39, 217, 213],
        "WarningColor": [143, 108, 151]
      },
      "programmingtools": {
        "CodeAnalyzerWarningColor": [143, 108, 151],
        "AutofixHighlightColor": [143, 108, 151],
        "VariableHighlightColor": [248, 0, 89],
        "VariablesWithSharedScopeColor": [91, 162, 182]
      }
    },
    "editor": {
