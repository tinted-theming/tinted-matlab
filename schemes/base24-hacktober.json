{
  "ColorSchemeName": "Hacktober",
  "ColorSchemeAuthor": "FredHappyface (https://github.com/fredHappyface)",
  "Note": "Apply this theme in Matlab using the tinted_import function, available at https://github.com/tinted-theming/tinted-matlab",

  "matlab": {
    "colors": {
      "ForegroundColor": [191, 189, 183],
      "BackgroundColor": [20, 20, 20],
      "KeywordColor": [134, 70, 81],
      "CommentColor": [93, 91, 89],
      "StringColor": [88, 119, 68],
      "UnterminatedStringColor": [89, 34, 28],
      "SystemCommandColor": [172, 145, 102],
      "SyntaxErrorColor": [179, 69, 56],
      "commandwindow": {
        "HyperlinkColor": [32, 110, 197],
        "ErrorColor": [179, 69, 56],
        "WarningColor": [83, 137, 197]
      },
      "programmingtools": {
        "CodeAnalyzerWarningColor": [83, 137, 197],
        "AutofixHighlightColor": [83, 137, 197],
        "VariableHighlightColor": [172, 145, 102],
        "VariablesWithSharedScopeColor": [208, 137, 73]
      }
    },
    "editor": {
