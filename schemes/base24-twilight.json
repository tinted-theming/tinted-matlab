{
  "ColorSchemeName": "Twilight",
  "ColorSchemeAuthor": "FredHappyface (https://github.com/fredHappyface)",
  "Note": "Apply this theme in Matlab using the tinted_import function, available at https://github.com/tinted-theming/tinted-matlab",

  "matlab": {
    "colors": {
      "ForegroundColor": [200, 200, 167],
      "BackgroundColor": [20, 20, 20],
      "KeywordColor": [180, 190, 123],
      "CommentColor": [92, 92, 81],
      "StringColor": [175, 185, 121],
      "UnterminatedStringColor": [96, 54, 33],
      "SystemCommandColor": [119, 130, 132],
      "SyntaxErrorColor": [192, 108, 67],
      "commandwindow": {
        "HyperlinkColor": [68, 70, 73],
        "ErrorColor": [192, 108, 67],
        "WarningColor": [90, 93, 97]
      },
      "programmingtools": {
        "CodeAnalyzerWarningColor": [90, 93, 97],
        "AutofixHighlightColor": [90, 93, 97],
        "VariableHighlightColor": [119, 130, 132],
        "VariablesWithSharedScopeColor": [194, 168, 108]
      }
    },
    "editor": {
