      "programmingtools": {
        "CodeAnalyzerWarningColor": [79, 194, 253],
        "AutofixHighlightColor": [103, 6, 11],
        "VariableHighlightColor": [92, 79, 73],
        "VariablesWithSharedScopeColor": [255, 202, 61]
      }
    },
